{
    "dataset_name": "MITMovie",
    "tasks": ["NER"],
    "dataloader_cls": "src.tasks.mitmovie.data_loader.MitMovieDatasetLoader",
    "sampler_cls": "src.tasks.mitmovie.data_loader.MitMovieSampler",
    "train_file": "data/MITmovie/engtrain.bio",
    "dev_file": "data/MITmovie/engdev.bio",
    "test_file": "data/MITmovie/engtest.bio",
    "few-shot_examples_from": "data/MITmovie/engtrain.bio",
    "prompt_template": "templates/user_prompt.j2",
    "parallel_instances": 15,
    "use_chat_format": true,
    "seed": [0, 24, 42],
    "label_noise_prob": [0.15, 0.50, 0.75],
    "task_configuration": {
        "NER": {
            "max_guidelines": -1,
            "guideline_dropout": 0.15,
            "scorer": "src.tasks.mitmovie.scorer.MitMovieEntityScorer",
            "paraphrase_train": true
        }
    }
}
